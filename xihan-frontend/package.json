    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "axios": "^1.6.5",
    "echarts": "^5.4.3",
    "naive-ui": "^2.37.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qrcode": "^1.5.3",
    "vfonts": "^0.0.3",
    "vue": "^3.4.13",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "eslint": "^8.56.0",
