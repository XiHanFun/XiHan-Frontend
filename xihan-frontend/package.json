  "keywords": [
    "Vue",
    "Vite",
    "TypeScript",
    "NaiveUI",
    "XiHan",
    "XiHanFun"
  ],
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite dev --host",
    "serve": "vite serve --host",
